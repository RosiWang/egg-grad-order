{
    "name": "grab-order",
    "version": "1.0.0",
    "description": "simple base egg grab order",
    "private": true,
    "egg": {
        "declarations": true
    },
    "dependencies": {
        "axios": "^0.19.0",
        "egg": "^2.15.1",
        "egg-scripts": "^2.11.0",
        "qs": "^6.7.0"
    },
    "devDependencies": {
        "autod": "^3.0.1",
        "autod-egg": "^1.1.0",
        "egg-bin": "^4.11.0",
        "egg-ci": "^1.11.0",
        "egg-mock": "^3.21.0",
        "eslint": "^5.13.0",
        "eslint-config-egg": "^7.1.0"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "scripts": {
        "start": "egg-scripts start --daemon --title=egg-server-grab-order",
        "stop": "egg-scripts stop --title=egg-server-grab-order",
        "dev": "egg-bin dev --port 7002",
        "debug": "egg-bin debug",
        "test": "npm run lint -- --fix && npm run test-local",
        "test-local": "egg-bin test",
        "cov": "egg-bin cov",
        "lint": "eslint .",
        "ci": "npm run lint && npm run cov",
        "autod": "autod"
    },
    "ci": {
        "version": "10"
    },
    "repository": {
        "type": "git",
        "url": ""
    },
    "author": "Rosi",
    "license": "MIT"
}
